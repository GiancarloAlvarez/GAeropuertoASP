@model IEnumerable<GAeropuerto.Models.Estadium>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAvionNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdHangarNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAvionNavigation.IdAvion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdHangarNavigation.CodigoHangar)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdEstadia">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdEstadia">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdEstadia">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
